{
  "manifest_version": 3,
  "name": "Virtual Judge Helper",
  "version": "1.2",
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "images/gray_icon.png"
    }
  }
}
